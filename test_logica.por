programa
{
	
	funcao inicio()
	{

		real num1
		real num2
		
		escreva("Digite o primeiro número: ")
		leia(num1)
		
		escreva("Digite o segundo número: ")
		leia(num2)

		escreva("\nPrimeiro número Digitado: " + num1)
		escreva("\nSegundo número Digitado:  " + num2)
	}
}
